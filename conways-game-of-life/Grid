pen_penDown
pen_penUp
go to x: (((-240) + ((GridWidth) - (OffsetX))) * (ZoomLevel)) y: (((180) - ((0) - (OffsetY))) * (ZoomLevel))
pen_penDown
pen_penUp
go to x: (((-240) + ((GridWidth) - (OffsetX))) * (ZoomLevel)) y: (((180) - ((GridHeight) - (OffsetY))) * (ZoomLevel))
pen_penDown
pen_penUp
pen_setPenSizeTo
set [Idx v] to [1]
repeat (length of [ActiveClusterIdx v]) 
