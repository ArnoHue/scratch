    {
        If ((Depth % 2) == 1)
        {
            IsWhiteChecked = TTProbeNode.CheckedRes;
        }
        Else
        {
            IsBlackChecked = TTProbeNode.CheckedRes;
        }
        Stop(this script);
    }
    If ((Depth % 2) == 1)
    {
        IsWhiteChecked = 0;
        Caller.Idx = 1;
        Repeat (64)
        {
            If (Board[Caller.Idx] > 0)
            {
                Call IsCheckingPiece(Caller.Idx);
                If (IsCheckingPieceRes == 1)
                {
                    IsWhiteChecked = 1;
                }
            }
            Caller.Idx += 1;
        }
        Call TTStoreNode(CurrentBoardHash)((Depth + 1))("")("")("")(IsWhiteChecked)(-1)("")("")("")("")("")("");
    }
    Else
    {
        IsBlackChecked = 0;
        Caller.Idx = 1;
        Repeat (64)
        {
            If (Board[Caller.Idx] < 0)
            {
                Call IsCheckingPiece(Caller.Idx);
                If (IsCheckingPieceRes == 1)
                {
                    IsBlackChecked = 1;
                }
            }
            Caller.Idx += 1;
        }
        Call TTStoreNode(CurrentBoardHash)((Depth + 1))("")("")("")(IsBlackChecked)(-1)("")("")("")("")("")("");
    }
}

