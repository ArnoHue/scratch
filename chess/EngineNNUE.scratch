                                                    IsLegalMove = 1;
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                        Else
                        {
                            If ((X == 0) Or (Y == 0))
                            {
                                If (IsSlidingPiece > 1)
                                {
                                    IsLegalMove = 1;
                                }
                            }
                            Else
                            {
                                If (X == Y)
                                {
                                    If (Not ((IsSlidingPiece == 2)))
                                    {
                                        IsLegalMove = 1;
                                    }
                                }
                            }
                        }
                    }
                    If ((IsLegalMove == 1) Or ((SourceIdx == 0) Or (TargetIdx == 0)))
                    {
                        Call GetPiecePosEval(SourcePiece)(TargetIdx);
                        Call GetPiecePosHash(SourcePiece)(TargetIdx);
                        If (SourcePiece > 0)
