                    VirtualMove = GoK_WhiteQueensideCastling;
                }
                Else
                {
                    If (Move == GoK_WhiteKingsideCastling)
                    {
                        VirtualMove = GoK_BlackKingsideCastling;
                    }
                    Else
                    {
                        If (Move == GoK_WhiteQueensideCastling)
