                            List.ReplaceItem(MoveToHashWhite, Idx, (PiecePosHash - PiecePosHash2));
                        }
                    }
                    TargetIdx += 1;
                }
                SourceIdx += 1;
            }
            Caller.Idx += 1;
        }
    }
}

