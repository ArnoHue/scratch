    {
        ProfilerMode += 1;
        IsBlockProfilerEnabled = 0;
        Call SayAsync("Statistical profiling enabled. Press 'l' to view data.")(2);
    }
    Else
    {
        If ((ProfilerMode % 10) == 1)
        {
            ProfilerMode += 1;
            IsBlockProfilerEnabled = 1;
            Call SayAsync("Full profiling enabled. Press 'l' / 'r' to view data.")(2);
