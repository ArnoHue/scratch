            {
                List.Add(CalcMovesScoreList, MaxMoveOrderScore);
            }
            Else
            {
                List.Add(CalcMovesScoreList, (0 - MaxMoveOrderScore));
            }
            Stop(this script);
        }
    }
    Call GetMoveScore("")("")("")("")(Move)(Depth);
    List.Add(CalcMovesScoreList, MoveScoreRes);
}

