    {
        Call TaperedEval.Add(0)((40 * BlackMinorCountLocal));
        Call TaperedEval.Add(0)((-40 * WhiteMinorCountLocal));
        Call TaperedEval.Add(0)((75 * BlackQueenCountLocal));
        Call TaperedEval.Add(0)((-75 * WhiteQueenCountLocal));
