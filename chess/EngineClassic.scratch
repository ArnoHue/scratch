        }
    }
    Else
    {
        If (IdxToRank[RookIdx] < 7)
        {
            Stop(this script);
        }
        IsTrapped = 1;
        If (IdxToRank[RookIdx] == 7)
        {
            If ((RookIdx == 52) Or (RookIdx == 53))
