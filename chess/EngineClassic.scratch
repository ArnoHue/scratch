            CurrentValue = Operator.Join(CurrentValue, MoveToZeroPadStr[((((BoardAttackedByBlack[(Idx + 0)] * 1000) + (BoardAttackedByBlack[(Idx + 1)] * 100)) + ((BoardAttackedByBlack[(Idx + 2)] * 10) + BoardAttackedByBlack[(Idx + 3)])) + 1)]);
