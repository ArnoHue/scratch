        {
            IdxSub = ((PieceMobility[(Idx + 0)] * 2) + 1);
            If (CurrentPieceAbs == GoK_BlackRook)
            {
                Call TaperedEval.Add((RookMobilityBonus[(IdxSub + 0)] * Factor))((RookMobilityBonus[(IdxSub + 1)] * Factor));
            }
            Else
            {
                If (CurrentPieceAbs == GoK_BlackBishop)
