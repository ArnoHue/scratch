    }
    If (BestMoves[Depth] == "")
    {
        If (MovesCount[Depth] > 0)
        {
            List.ReplaceItem(MovesIdx, Depth, 1);
            Call GetCurrentMove(Depth);
            List.ReplaceItem(BestMoves, Depth, CurrentMove);
