            Idx += 4;
        }
        List.ReplaceItem(BoardAttackedByWhiteSnapshot, StoreDepth, CurrentValue);
        List.ReplaceItem(BoardAttackedByWhitePieceSnapshot, StoreDepth, CurrentValue2);
        If ((StoreDepth % 2) == 1)
        {
            List.ReplaceItem(CheckedState, StoreDepth, IsBlackChecked);
