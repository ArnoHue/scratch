Define TTStoreBoard(string BoardHash)(string BoardScore1)(string BoardScore2)(string BoardScore3)(string BoardScoreSum) (warp=true)
{
    HashBucket = ((BoardHash % (List.Length(TTBoardHash) - 5)) + 1);
    Idx = 1;
    Repeat Until (((TTBoardHash[(HashBucket + 0)] == BoardHash) Or (TTBoardHash[(HashBucket + 0)] == 0)) Or (Idx > 4))
    {
        Idx += 1;
        HashBucket += 1;
    }
    If (Not (((TTBoardHash[(HashBucket + 0)] == BoardHash) Or (TTBoardHash[(HashBucket + 0)] == 0))))
    {
        HashBucket = ((BoardHash % (List.Length(TTBoardHash) - 5)) + 1);
        Idx = 1;
        Repeat Until (TTBoardAge[(HashBucket + 0)] < (TTCurrentAge - 1))
        {
            If (Idx > 4)
            {
                Stop(this script);
            }
            Idx += 1;
            HashBucket += 1;
        }
    }
    If (Not ((TTBoardHash[(HashBucket + 0)] == BoardHash)))
    {
        List.ReplaceItem(TTBoardAge, HashBucket, TTCurrentAge);
        List.ReplaceItem(TTBoardHash, HashBucket, BoardHash);
        List.ReplaceItem(TTBoardScore1, HashBucket, BoardScore1);
        List.ReplaceItem(TTBoardScore2, HashBucket, BoardScore2);
        List.ReplaceItem(TTBoardScore3, HashBucket, BoardScore3);
        List.ReplaceItem(TTBoardScoreSum, HashBucket, BoardScoreSum);
    }
    Else
    {
        If (Not ((BoardScore1 == "")))
        {
            List.ReplaceItem(TTBoardScore1, HashBucket, BoardScore1);
        }
        If (Not ((BoardScore2 == "")))
        {
            List.ReplaceItem(TTBoardScore2, HashBucket, BoardScore2);
        }
        If (Not ((BoardScore3 == "")))
        {
            List.ReplaceItem(TTBoardScore3, HashBucket, BoardScore3);
        }
        If (Not ((BoardScoreSum == "")))
        {
            List.ReplaceItem(TTBoardScoreSum, HashBucket, BoardScoreSum);
        }
    }
}

