                    EvalExtendedResult2 += (10 * ((BlackQueenCountLocal * 2) + 1));
                }
            }
        }
    }
    If (BlackBishopCountLocal > 1)
    {
        EvalExtendedResult2 += 25;
    }
    If (WhiteBishopCountLocal > 1)
    {
        EvalExtendedResult2 += -25;
    }
}

Define EvaluateBoardExtended3(string Depth) (warp=true)
{
    EvalExtendedResult3 = 0;
    CountOuter1 = ((WhiteQueenCountLocal * 9) + (WhiteMinorCountLocal * 4));
    CountOuter2 = ((BlackQueenCountLocal * 9) + (BlackMinorCountLocal * 4));
    If (CountOuter1 > CountOuter2)
    {
        CountOuter1 = CountOuter2;
    }
    If ((IsEndgame == 1) Or (CountOuter1 < 10))
    {
        Stop(this script);
    }
    Call TTProbeBoard(CurrentBoardHash);
    If ((TTProbeRes == 1) And (Not ((TTProbeBoardScore3Res == ""))))
    {
        EvalExtendedResult3 = TTProbeBoardScore3Res;
        Stop(this script);
    }
    Call EvaluateBoardExtended3Impl(Depth);
    If (EvalExtendedResult3 > 0)
    {
        CurrentValue = ((EvalExtendedResult3 * EvalExtendedResult3) / 2048);
    }
    Else
    {
        CurrentValue = (0 - ((EvalExtendedResult3 * EvalExtendedResult3) / 2048));
    }
    CountOuter1 = ((WhiteQueenCountLocal * 9) + (WhiteMinorCountLocal * 4));
    CountOuter2 = ((BlackQueenCountLocal * 9) + (BlackMinorCountLocal * 4));
    If (CountOuter1 > CountOuter2)
    {
        CountOuter1 = CountOuter2;
    }
    CountOuter1 += -9;
    EvalExtendedResult3 = Operator.Round(((CurrentValue * (CountOuter1 / 24)) + (0 * ((24 - CountOuter1) / 24))));
    If (1 == 0)
    {
        EvalExtendedResult3 = Operator.Round(((EvalExtendedResult3 * 0.5) + ((EvalExtendedResult3 * 0.5) * ((24 - CountOuter1) / 24))));
    }
    Call TTStoreBoard(CurrentBoardHash)("")("")(EvalExtendedResult3)("");
}

Define EvaluateBoardExtended3Impl(string Depth) (warp=true)
{
    EvalExtendedResult3 = 0;
    List.DeleteAll(TmpList1);
    List.DeleteAll(TmpList2);
    List.DeleteAll(TmpList3);
    List.DeleteAll(TmpList4);
    List.DeleteAll(TmpList5);
    Side = 1;
    Repeat (2)
    {
        If (Side == 1)
        {
            KingIdx = BlackKingIdx;
        }
        Else
        {
            KingIdx = WhiteKingIdx;
        }
        Rank = (IdxToRank[KingIdx] - 1);
        Repeat (3)
        {
            If ((Rank > 0) And (Rank < 9))
            {
                File = (IdxToFile[KingIdx] - 1);
                Repeat (3)
                {
                    If ((File > 0) And (File < 9))
                    {
                        If ((Not ((File == IdxToFile[KingIdx]))) Or (Not ((Rank == IdxToRank[KingIdx]))))
                        {
                            Idx = ((Rank * 8) + File);
                            If (Side == 1)
                            {
                                List.Add(TmpList1, Idx);
                            }
                            Else
                            {
                                List.Add(TmpList2, Idx);
                            }
                            If (Not (List.ContainsItem(TmpList3, Idx)))
                            {
                                List.Add(TmpList3, Idx);
                            }
                        }
                    }
                    File += 1;
                }
            }
            Rank += 1;
        }
        Side += 1;
    }
    Call ClearBoardAttackedByMove((Depth + 1));
    Call ClearBoardAttackedByMove((Depth + 2));
    SourceIdx = 1;
    Repeat (64)
    {
        CurrentPiece = Board[SourceIdx];
        CurrentPieceAbs = abs(CurrentPiece);
        If ((CurrentPieceAbs > GoK_BlackPawn) And (CurrentPieceAbs < GoK_BlackKing))
        {
            Prefix = ((PieceIdxLookup[CurrentPieceAbs] * 10000) + (SourceIdx * 100));
            Idx = 1;
            Repeat (List.Length(TmpList3))
            {
                TargetIdx = TmpList3[Idx];
                Found = 0;
                If (CurrentPiece > 0)
                {
                    If (Not ((MovePosLookupBlack[(Prefix + TargetIdx)] == "")))
                    {
                        Found = 1;
                    }
                }
                Else
                {
                    If (Not ((MovePosLookupWhite[(Prefix + TargetIdx)] == "")))
                    {
                        Found = 1;
                    }
                }
                If (Found == 1)
                {
                    If (CurrentPieceAbs > GoK_BlackKnight)
                    {
                        LookupIdx = ((SourceIdx * 100) + TargetIdx);
                        SquareIdx = SourceIdx;
                        Repeat ((MovePathDelta[LookupIdx] - 2))
                        {
                            SquareIdx += MovePathStepInc[LookupIdx];
                            If (Not ((Board[SquareIdx] == 0)))
                            {
                                Found = 0;
                            }
                        }
                    }
                    If (Found == 1)
                    {
                        If (CurrentPiece > 0)
                        {
                            List.ReplaceItem(BoardAttackedByBlack, TargetIdx, (BoardAttackedByBlack[TargetIdx] + 1));
                            CurrentValue = 0;
                            If ((BoardAttackedByBlackPiece[TargetIdx] == 0) Or (PieceIdxLookup[CurrentPieceAbs] < PieceIdxLookup[abs(Board[BoardAttackedByBlackPiece[TargetIdx]])]))
                            {
                                List.ReplaceItem(BoardAttackedByBlackPiece, TargetIdx, SourceIdx);
                            }
                        }
                        Else
                        {
                            List.ReplaceItem(BoardAttackedByWhite, TargetIdx, (BoardAttackedByWhite[TargetIdx] + 1));
                            CurrentValue = 0;
                            If ((BoardAttackedByWhitePiece[TargetIdx] == 0) Or (PieceIdxLookup[CurrentPieceAbs] < PieceIdxLookup[abs(Board[BoardAttackedByWhitePiece[TargetIdx]])]))
                            {
                                List.ReplaceItem(BoardAttackedByWhitePiece, TargetIdx, SourceIdx);
                            }
                        }
                    }
                }
                Idx += 1;
            }
        }
        SourceIdx += 1;
    }
    List.DeleteAll(KingRingAttackersCount);
    List.DeleteAll(KingRingAttacksCount);
    List.DeleteAll(KingRingAttacksWeight);
    List.DeleteAll(KingRingAttacksOnWeakCount);
    List.DeleteAll(KingRingAttacksBishopDefenders);
    List.DeleteAll(KingRingAttacksKnightDefenders);
    Repeat (2)
    {
        List.Add(KingRingAttackersCount, 0);
        List.Add(KingRingAttacksCount, 0);
        List.Add(KingRingAttacksWeight, 0);
        List.Add(KingRingAttacksOnWeakCount, 0);
        List.Add(KingRingAttacksBishopDefenders, 0);
        List.Add(KingRingAttacksKnightDefenders, 0);
    }
    List.DeleteAll(AttacksWeight);
    List.Add(AttacksWeight, 0);
    List.Add(AttacksWeight, 36);
    List.Add(AttacksWeight, 26);
    List.Add(AttacksWeight, 21);
    List.Add(AttacksWeight, 5);
    List.Add(AttacksWeight, 0);
    Side = 1;
    Repeat (2)
    {
        List.DeleteAll(TmpList4);
        If (Side == 1)
        {
            CountOuter1 = List.Length(TmpList1);
        }
        Else
        {
            CountOuter1 = List.Length(TmpList2);
        }
        IdxOuter = 1;
        Repeat (CountOuter1)
        {
            IsWeak = 0;
            If (Side == 1)
            {
                Idx = TmpList1[IdxOuter];
                Count1 = BoardAttackedByWhite[Idx];
                IdxSub = BoardAttackedByWhitePiece[Idx];
                DefenderPiece = Board[BoardAttackedByBlackPiece[Idx]];
                If ((BoardAttackedByBlack[Idx] == 0) Or (DefenderPiece == GoK_BlackQueen))
                {
                    IsWeak = 1;
                }
            }
            Else
            {
                Idx = TmpList2[IdxOuter];
                Count1 = BoardAttackedByBlack[Idx];
                IdxSub = BoardAttackedByBlackPiece[Idx];
                DefenderPiece = Board[BoardAttackedByWhitePiece[Idx]];
                If ((BoardAttackedByWhite[Idx] == 0) Or (DefenderPiece == GoK_WhiteQueen))
                {
                    IsWeak = 1;
                }
            }
            If (Count1 > 0)
            {
                List.ReplaceItem(KingRingAttacksCount, Side, (KingRingAttacksCount[Side] + 1));
                List.ReplaceItem(KingRingAttacksWeight, Side, (KingRingAttacksWeight[Side] + AttacksWeight[PieceIdxLookup[abs(Board[IdxSub])]]));
                If (IsWeak == 1)
                {
                    List.ReplaceItem(KingRingAttacksOnWeakCount, Side, (KingRingAttacksOnWeakCount[Side] + 1));
                }
                If (Not (List.ContainsItem(TmpList4, IdxSub)))
                {
                    List.ReplaceItem(KingRingAttackersCount, Side, (KingRingAttackersCount[Side] + 1));
                    List.Add(TmpList4, IdxSub);
                }
                If (abs(DefenderPiece) == GoK_BlackBishop)
                {
                    List.ReplaceItem(KingRingAttacksBishopDefenders, Side, (KingRingAttacksBishopDefenders[Side] + 1));
                }
                Else
                {
                    If (abs(DefenderPiece) == GoK_BlackKnight)
                    {
                        List.ReplaceItem(KingRingAttacksKnightDefenders, Side, (KingRingAttacksKnightDefenders[Side] + 1));
                    }
                }
            }
            IdxOuter += 1;
        }
        CurrentValue = 0;
        CurrentValue += (KingRingAttacksWeight[Side] * KingRingAttackersCount[Side]);
        CurrentValue += (32 * KingRingAttacksCount[Side]);
        CurrentValue += (86 * KingRingAttacksOnWeakCount[Side]);
        CurrentValue += (47 * KingRingAttacksKnightDefenders[Side]);
        CurrentValue += (18 * KingRingAttacksBishopDefenders[Side]);
        If (Side == 1)
        {
            EvalExtendedResult3 += (0 - CurrentValue);
        }
        Else
        {
            EvalExtendedResult3 += CurrentValue;
        }
        Side += 1;
    }
    If (BlackQueenCountLocal == 0)
    {
        EvalExtendedResult3 += -300;
    }
    If (WhiteQueenCountLocal == 0)
    {
        EvalExtendedResult3 += 300;
    }
}

